mport random

number = random.randint(1, 50)
guess = 0

while guess != number:
    guess = int(input("Enter your guess (1-50): "))

    if guess < number:
        print("Too low!")
    elif guess > number:
        print("Too high!")
    else:
        print("Congratulations! You guessed it right ğŸ‰")
